<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.golaxy.xwtk.mapper.UserMapper">
    <select id="findList" resultType="cn.golaxy.xwtk.entity.User">
        select  * from user
    </select>
    <select id="selectUserPage" resultType="cn.golaxy.xwtk.entity.User">
        select * from user where grade = #{grade}
    </select>
</mapper>